#include<iostream>
using namespace std;
int main()
{

    char grade;
    cin >> grade;

    switch (grade)
    {
    case 'A':
        cout << "excellent";
        break;
    case 'a':
        cout << "excellent";
        break;
    case 'B':
        cout << "very good";
        break;
    case 'b':
        cout << "very good";
        break;
    default:
        cout << "invalid cha";
        break;

    }
    return 0;
}
